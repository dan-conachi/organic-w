{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "dist",
        "sourceMap": true,
        "types": ["node"],
        "experimentalDecorators": true,
        "allowJs": true,
        "typeRoots": ["node_modules/@types", "custom_typings"]
    },
    "include": [
        "**/*.ts"
    ],
    "exclude": [
        "node_modules", "webapp"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true
}